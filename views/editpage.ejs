<div id="mymodal" class="main-content">
    <div id="modal-content">

            <span class="modal-close" >&times;</span>
            <form action="/admin/updateSong/<%= song._id %>" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="">Upload Image</label>
                    <input type="file" name="images" id="Ã¯mages">
                </div>
                <div class="form-group">
                    <input value="<%= song.title %>" type="text" placeholder="Enter Title" name="title" id="utitle">
                </div>
                <div class="form-group">
                    <label for="">Upload Song</label>
                    <input type="file" name="mp3" id="mp3">
                </div>
                <div class="form-group">
                    <select name="genre" id="genre" value="<%= song.genre %>">
                        <option>Select Genre</option>
                        <% for(let i = 0; i < genre.length; i++) { %>
                            <option value="<%= genre[i].title %>"><%= genre[i].title %></option>
                        <% } %>
                    </select>
                </div>
                <div class="form-group">
                    <textarea id="myTextarea" name="lyrics" placeholder="Enter Lyrics" rows="20" cols="20"><%= song.lyrics %></textarea>
                </div>
                <input type="submit" value="Upload">
            </form>

        
    </div>

</div>


       
       
  

  
             